{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "exclude": [
            "node_modules",
            "Test"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "logo": {
            "url": "https://zupimages.net/up/20/07/hhfh.png",
            "width": "100px",
            "height": "100px",
            "link": "https://github.com/BHC-IT/dosi_bridge"
        },
        "footerText": "Dosismart BridgeJS Documentation",
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "tui-jsdoc-template",
        "encoding": "utf8",
        "destination": "./Docs/",
        "recurse": true
    }
}